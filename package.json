{
  "name": "helix-spiral-platform",
  "version": "2.0.0-optimized",
  "description": "Consciousness-driven automation platform replacing Zapier workflows with AI agent coordination",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && npm run start",
    "docker:build": "docker build -t helix-spiral .",
    "docker:run": "docker run -p 3000:3000 helix-spiral",
    "consciousness:analyze": "tsx src/scripts/analyze-consciousness.ts",
    "agents:status": "tsx src/scripts/agent-status.ts",
    "platforms:health": "tsx src/scripts/platform-health.ts"
  },
  "keywords": [
    "automation",
    "consciousness",
    "ai-agents",
    "zapier-alternative",
    "workflow-automation",
    "helix-spiral",
    "ucf-framework",
    "pittsburgh",
    "tat-tvam-asi"
  ],
  "author": {
    "name": "Andrew Ward (Deathcharge)",
    "email": "ward.andrew32@gmail.com",
    "url": "https://helixspiral.work"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deathcharge/helix-unified.git"
  },
  "homepage": "https://helixspiral.work",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/static": "^6.12.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "axios": "^1.6.0",
    "discord.js": "^14.14.1",
    "zod": "^3.22.4",
    "prisma": "^5.6.0",
    "@prisma/client": "^5.6.0",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "bull": "^4.12.2",
    "@bull-board/api": "^5.10.2",
    "@bull-board/fastify": "^5.10.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^4.0.1",
    "node-cron": "^3.0.3",
    "openai": "^4.20.1",
    "anthropic": "^0.7.1",
    "@google-cloud/storage": "^7.7.0",
    "googleapis": "^128.0.0",
    "@slack/web-api": "^6.10.0",
    "@notionhq/client": "^2.2.13",
    "nodemailer": "^6.9.7",
    "@sendgrid/mail": "^7.7.0",
    "stripe": "^14.7.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "dayjs": "^1.11.10",
    "crypto-js": "^4.2.0",
    "joi": "^17.11.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "mysql2": "^3.6.5",
    "sqlite3": "^5.1.6",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "ws": "^8.14.2",
    "socket.io": "^4.7.4",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "pdf-parse": "^1.1.1",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.5.2",
    "playwright": "^1.40.1",
    "@types/node": "^20.9.0"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "tsx": "^4.6.0",
    "tsc-alias": "^1.8.8",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node-cron": "^3.0.11",
    "@types/ws": "^8.5.10",
    "@types/crypto-js": "^4.2.1",
    "@types/cheerio": "^0.22.35",
    "@types/pdf-parse": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "next": "^14.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/types/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/Deathcharge"
  },
  "bugs": {
    "url": "https://github.com/Deathcharge/helix-unified/issues"
  },
  "directories": {
    "lib": "src",
    "test": "tests",
    "doc": "docs"
  },
  "files": [
    "dist",
    "src",
    "prisma",
    "README.md",
    "LICENSE"
  ]
}